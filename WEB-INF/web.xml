<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
	"http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
	<servlet>
		<servlet-name>WebService</servlet-name>
		<servlet-class>WebService</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>WebService</servlet-name>
		<url-pattern>/response</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>InterCarte</servlet-name>
		<servlet-class>InterCarte</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InterCarte</servlet-name>
		<url-pattern>/Maps</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
